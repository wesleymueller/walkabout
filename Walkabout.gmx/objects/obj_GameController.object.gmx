        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Move player
if !global.paused &amp;&amp; !global.barf {

    if (hdir != 0) {
        moveSpeed += playerAccel * hdir
        moveSpeed = clamp(moveSpeed, -maxSpeed, maxSpeed);
    }
    
    if (hdir == 0) {
        lastSpeed = moveSpeed;
        moveSpeed -= playerAccel * sign(moveSpeed);
        if (moveSpeed != 0) &amp;&amp; (sign(moveSpeed) != sign(lastSpeed)) {
            moveSpeed = 0;
        }
        
    }
    moveSpeed = round(moveSpeed);
    lastSpeed = moveSpeed;
    obj_player.x += moveSpeed;
    global.playerx = obj_player.x;
    
    obj_player.x = clamp(obj_player.x, (obj_player.sprite_width / 2), room_width - (obj_player.sprite_width / 2))

}

if global.barf {
    moveSpeed = 0;
}  
</string>
