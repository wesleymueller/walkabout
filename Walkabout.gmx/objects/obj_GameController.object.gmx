        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Set sprite

if !global.paused &amp;&amp; !global.barf &amp;&amp; !global.teleporting {
    if (hdir &lt; 0) {
        obj_player.sprite_index = spr_PlayerLeft;
    }
    
    if (hdir &gt; 0) {
        obj_player.sprite_index = spr_PlayerRight;
    }
    
    if (hdir == 0) {
        if lastSpeed &lt; 0 {
            obj_player.sprite_index = spr_PlayerStandingLeft;   
        }
        if lastSpeed &gt; 0 {
            obj_player.sprite_index = spr_PlayerStandingRight;   
        }
    }
    if (sign(lastSpeed) != sign(hdir)) {
        if (hdir == -1) {
            obj_player.sprite_index = spr_PlayerStandingRight; 
        }
        if (hdir == 1) {
            obj_player.sprite_index = spr_PlayerStandingLeft;
        }
    }
}

if global.teleporting {
    if lastSpeed &lt; 0 {
        obj_player.sprite_index = spr_TeleportLeft;   
    }
    if lastSpeed &gt; 0 {
        obj_player.sprite_index = spr_TeleportRight;   
    }
    if lastSpeed == 0 {
        obj_player.sprite_index = spr_TeleportRight;
    }
}
</string>
