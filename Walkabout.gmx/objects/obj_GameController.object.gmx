        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Time Travel

gp_Past = gamepad_button_check_pressed(0, gp_face2);
gp_Future = gamepad_button_check_pressed(0, gp_face3);
key_Past = keyboard_check_pressed(ord('X'));
key_Future = keyboard_check_pressed(ord('C'));

if !global.paused {
    if (global.teleportCooldown == 0) {
        
        if (gp_Past || key_Past) {
            global.teleportDirection = -1;
            if !((global.currentRoom + global.teleportDirection &gt; 2) || (global.currentRoom + global.teleportDirection &lt; 0)) {
                global.teleporting = 1;
            }
        }
        
        if (gp_Future || key_Future) {
            global.teleportDirection = 1;
            if !((global.currentRoom + global.teleportDirection &gt; 2) || (global.currentRoom + global.teleportDirection &lt; 0)) {
                global.teleporting = 1;
            }
        }
        
    } else {
        global.teleportCooldown--;
        if global.teleportCooldown &lt; 0 {
            global.teleportCooldown = 0;
        }
    }
}
if (global.teleportDirection != 0) &amp;&amp; ((obj_player.sprite_index == spr_TeleportLeft) || (obj_player.sprite_index == spr_TeleportRight)) {
    hdir = 0;
    if (obj_player.image_index &gt;= obj_player.image_number - 1) {
        
        if !((global.currentRoom + global.teleportDirection &gt; 2) || (global.currentRoom + global.teleportDirection &lt; 0)) {
            global.teleportCooldown = global.maxCoolDown;
            room_goto(global.rooms[global.currentRoom + global.teleportDirection]);
        }
    }
}
</string>
