keydown = keyboard_check_pressed(vk_down);
keyup = keyboard_check_pressed(vk_up);
gpaxis = gamepad_axis_value(0, gp_axislv);
gadown = 0;
gaup = 0;

axishold = 0;

if (gpaxis &gt; 0.1) &amp;&amp; (axishold == 0) {
    axishold = 1;
    gaup = 1;

    
}
if (gpaxis &lt; -0.1) &amp;&amp; (axishold == 0) {
    axishold = 1;
    gadown = 1;
}
