    global.cursorPosition++;
    if global.cursorPosition &gt; array_length_1d(global.menu) - 1 {
        global.cursorPosition = 0;
    }
}

if (gpup || keyup || gaup) {
    global.cursorPosition--;
    if global.cursorPosition &lt; 0 {
        global.cursorPosition = array_length_1d(global.menu) - 1;
    }
}
